{
    "manifest_version": 3,
    "name": "Banana Extension",
    "short_name": "Banana",
    "version": "1.0",
    "author": "clevercart",
    "description": "talk to pdf",
    "background": {
        "service_worker": "background.js"
    }, "icons": {
        "128": "APA.png"
      },
    "content_scripts": [
        {
            "matches": ["https://extensionpay.com/*"],
            "js": ["ExtPay.js"],
            "run_at": "document_start"
        },
        {
            "matches": ["http://*/", "https://*/"],
            "js": ["ExtPay.js", "content.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ]
}

